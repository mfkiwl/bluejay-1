#===============================================
# Project Directories 
#===============================================
BLUEJAY := /home/seankent/bluejay
TOOLS := $(BLUEJAY)/tools
BUILD := $(BLUEJAY)/build
SRC := $(BLUEJAY)/src
TB := $(BLUEJAY)/tb

#===============================================
# Rules
#===============================================
#===============================
# all
#===============================
all: $(SRC) 

#===============================
# $(SRC) 
#===============================
.PHONY: $(SRC)
$(SRC): 
	$(MAKE) -C $@

#===============================
# clean 
#===============================
.PHONY: clean 
clean: clean_src 

#===============================
# clean_src 
#===============================
.PHONY: clean_src 
clean_src:
	$(MAKE) -C $(SRC) clean

